<Window
    x:Class="AssemblyUpdater.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AssemblyUpdater"
   xmlns:conv="clr-namespace:AssemblyUpdater.Converter"
    xmlns:uc="clr-namespace:AssemblyUpdater.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="MainWindowView"
    Title="NET Framework Projects Manager"
    Width="700"
    Height="530"
    mc:Ignorable="d">


    <TabControl>
        <TabItem Header="Assembly Info">
            <uc:AssemblyInfo/>
        </TabItem>
        <TabItem Header="Net Framework">
            <uc:NetFramework/>
        </TabItem>
    </TabControl>

</Window>
